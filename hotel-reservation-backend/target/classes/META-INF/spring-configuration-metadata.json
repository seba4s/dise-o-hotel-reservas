{
  "groups": [
    {
      "name": "jwt",
      "type": "com.hotel.reservation.config.JwtConfig",
      "sourceType": "com.hotel.reservation.config.JwtConfig"
    }
  ],
  "properties": [
    {
      "name": "jwt.audience",
      "type": "java.lang.String",
      "description": "JWT audience",
      "sourceType": "com.hotel.reservation.config.JwtConfig",
      "defaultValue": "hotel-users"
    },
    {
      "name": "jwt.expiration",
      "type": "java.lang.Long",
      "description": "JWT token expiration time in milliseconds (24 hours)",
      "sourceType": "com.hotel.reservation.config.JwtConfig",
      "defaultValue": 86400000
    },
    {
      "name": "jwt.header-string",
      "type": "java.lang.String",
      "description": "JWT token header name",
      "sourceType": "com.hotel.reservation.config.JwtConfig",
      "defaultValue": "Authorization"
    },
    {
      "name": "jwt.issuer",
      "type": "java.lang.String",
      "description": "JWT issuer",
      "sourceType": "com.hotel.reservation.config.JwtConfig",
      "defaultValue": "hotel-reservation-system"
    },
    {
      "name": "jwt.refresh-expiration",
      "type": "java.lang.Long",
      "description": "JWT refresh token expiration time in milliseconds (7 days)",
      "sourceType": "com.hotel.reservation.config.JwtConfig",
      "defaultValue": 604800000
    },
    {
      "name": "jwt.secret",
      "type": "java.lang.String",
      "description": "JWT secret key for signing tokens",
      "sourceType": "com.hotel.reservation.config.JwtConfig",
      "defaultValue": "hotelReservationSecretKey2025VerySecureAndLongKey"
    },
    {
      "name": "jwt.token-prefix",
      "type": "java.lang.String",
      "description": "JWT token prefix for Authorization header",
      "sourceType": "com.hotel.reservation.config.JwtConfig",
      "defaultValue": "Bearer "
    }
  ],
  "hints": []
}